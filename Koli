StarterGui:SetCore("SendNotification", {
    Title = "ğŸš¨ ØªØ·ÙˆÙŠØ± ÙŠÙˆØ³Ù Ø§Ù„Ø²Ù†ÙƒÙ„ÙˆÙ†ÙŠ ğŸš¨",
    Text = "Loaded",
    Duration = 5
})

local function checkMessage(player, message)
    local lowered = string.lower(message)
    for _, badWord in ipairs(badWords) do
        if string.find(lowered, badWord) then
            StarterGui:SetCore("SendNotification", {
                Title = "ØªØ·ÙˆÙŠØ± ÙŠÙˆØ³Ù Ø§Ù„Ø²Ù†ÙƒÙ„ÙˆÙ†ÙŠ",
                Text = "ØªÙ… Ø§ÙƒØªØ´Ø§Ù Ø¥Ø³Ø§Ø¡Ø©\n"..player.Name..": "..message,
                Duration = 8
            })
            print("Ø¨Ù„Ø§Øº ØªÙ„Ù‚Ø§Ø¦ÙŠ: "..player.Name.." Ù‚Ø§Ù„ ÙƒÙ„Ù…Ø© Ù…Ø³ÙŠØ¦Ø©. Ø§Ù„Ø³Ø¨Ø¨: The player used obscene and offensive sexual language in chat.")
        end
    end
end

local function onPlayerAdded(player)
    player.Chatted:Connect(function(message)
        checkMessage(player, message)
    end)
end

Players.PlayerAdded:Connect(onPlayerAdded)

for _, player in ipairs(Players:GetPlayers()) do
    onPlayerAdded(player)
end
